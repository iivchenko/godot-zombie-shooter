[sub_resource type="GDScript" id=1]
script/source = "extends Control

onready var _button_hover = $Sounds/ButtonHover
onready var _button_click = $Sounds/ButtonClick

export var _next_scene = \"\"

func _ready() -> void: 
		Input.set_mouse_mode(Input.MOUSE_MODE_VISIBLE)
		get_tree().paused = false


func _on_start_sressed() -> void:
	_button_click.play()
	get_tree().change_scene(_next_scene)


func _on_exit_pressed() -> void:
	_button_click.play()
	get_tree().quit()


func _on_button_hover() -> void:
	_button_hover.play()
"

